{
    "manifest_version": 2,
    "name": "Color Extractor",
    "version": "1.0",
    "description": "Extract color values from web page stylesheets.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  